{
  "name": "blockid",
  "private": true,
  "version": "0.1.0-alpha.23",
  "description": "Block ID JavaScript SDK",
  "author": {
    "name": "Stanislaw Glogowski",
    "email": "hello@newobject.pl"
  },
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/blockid/sdk-javascript.git"
  },
  "bugs": {
    "url": "https://github.com/blockid/sdk-javascript/issues"
  },
  "scripts": {
    "prebuild": "rimraf ./dist/*",
    "build": "tsc",
    "postbuild": "tempack ./dist/",
    "lint": "tslint --exclude node_modules **/*.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublish": "npm run build",
    "publish": "cd ./dist && npm publish"
  },
  "dependencies": {
    "bn.js": "^4.11.8",
    "cross-fetch": "^2.2.2",
    "ethjs": "^0.4.0",
    "keccak": "^1.4.0",
    "protobufjs": "^6.8.8",
    "rxjs": "^6.2.2",
    "secp256k1": "^3.5.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/node": "^10.5.4",
    "jest": "^23.4.2",
    "rimraf": "^2.6.2",
    "tempack": "0.0.1",
    "ts-jest": "^23.0.1",
    "tslint": "^5.11.0",
    "typescript": "^2.9.2"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(\\.spec)\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
