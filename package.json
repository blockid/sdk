{
  "name": "blockid",
  "private": true,
  "version": "0.0.0",
  "description": "Block ID SDK",
  "author": {
    "name": "Stanislaw Glogowski",
    "email": "hello@newobject.pl"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/blockid/sdk.git"
  },
  "bugs": {
    "url": "https://github.com/blockid/sdk/issues"
  },
  "scripts": {
    "clean": "rimraf ./dist/*",
    "precompile": "npm run clean",
    "compile": "tsc",
    "compile:watch": "tsc --watch",
    "postcompile": "tempack ./dist/",
    "prebuild": "npm run lint && npm run test",
    "build": "npm run compile",
    "lint": "tslint src/**/*.ts",
    "test": "jest",
    "test:watch": "jest --watch --verbose false"
  },
  "dependencies": {
    "bn.js": "^4.11.8",
    "cross-fetch": "^2.2.2",
    "deep-equal-extended": "0.0.1",
    "eth-utils": "0.1.0-alpha.8",
    "ethereumjs-tx": "^1.3.7",
    "ethjs": "^0.4.0",
    "ethjs-abi": "^0.2.1",
    "protobufjs": "^6.8.8",
    "rxjs": "6.3.2",
    "rxjs-addons": "0.0.2",
    "ts-node": "^7.0.1"
  },
  "devDependencies": {
    "@types/jest": "^23.3.2",
    "@types/node": "^10.10.0",
    "jest": "^23.6.0",
    "rimraf": "^2.6.2",
    "tempack": "^1.1.0",
    "ts-jest": "^23.1.4",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(\\.spec)\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  }
}
